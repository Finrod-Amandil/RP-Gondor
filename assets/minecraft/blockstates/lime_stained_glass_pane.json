{
    "multipart": [
        {   "when": { "north": true },
            "apply": { "model": "glass_pane_half_right_lime" }
        },
        {   "when": { "east": true },
            "apply": { "model": "glass_pane_half_right_lime", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "glass_pane_half_left_lime" }
        },
        {   "when": { "west": true },
            "apply": { "model": "glass_pane_half_left_lime", "y": 90 }
        },
		{   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "unused_cross_0" }
        }
    ]
}
